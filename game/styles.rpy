# This file defines the styles and colors

# The number after init is the priority order (the lower the number, the higher the priority)
# say_label is a built-in style that displays text in a label format
init -2:
    style name_mc is say_label:
      color "#c8ffc8"
    style name_gk is say_label:
      color "#a8d8ff"
    style name_dn is say_label:
      color "#ffd8a8"
    style name_pl is say_label:
      color "#ffa8d8"
    style name_duyen is say_label:
      color "#d8a8ff"
    style name_unknown is say_label:
      color "#c8c8c8"
    style name_pl_dn is say_label:
      color "#ffb86c"